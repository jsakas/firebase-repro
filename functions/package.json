{
  "name": "functions",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "serve": "FIRESTORE_EMULATOR_HOST=localhost:8080 firebase emulators:start --only functions,firestore",
    "shell": "yarn build && firebase functions:shell",
    "start": "yarn shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log"
  },
  "engines": {
    "node": "14"
  },
  "main": "lib/index.js",
  "dependencies": {
    "firebase": "^9.0.2",
    "firebase-admin": "^9.8.0",
    "firebase-functions": "^3.14.1"
  },
  "devDependencies": {
    "firebase-functions-test": "^0.2.0",
    "typescript": "^3.8.0"
  },
  "private": true
}
